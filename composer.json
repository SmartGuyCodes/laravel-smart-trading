{
    "name": "smartguycodes/laravel-smart-trading",
    "description": "A Laravel package for smart trading functionalities.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SmartGuyCodes\\LaravelSmartTrading\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "SmartGuyCodes",
            "email": "smartguycodes@gmail.com",
            "role": "Developer",
            "homepage": "https://smartguycodes.co.ke"
        }
    ]
}